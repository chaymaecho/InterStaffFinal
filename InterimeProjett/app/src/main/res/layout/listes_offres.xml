<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal">


        <LinearLayout
            android:id="@+id/linearColumndeveloppeurweb"
            style="@style/groupStylegray_1"
            android:layout_width="366dp"
            android:layout_height="140dp"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_weight="1.0"
            android:gravity="start"
            android:background="@drawable/vertjaun"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linearRowdeveloppeurweb"
                android:layout_width="355dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_11pxh"
                android:gravity="start"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/eveloppeurweb2"
                    android:layout_width="230dp"
                    android:layout_height="130dp"
                    android:gravity="start"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtDeveloppeurWeb"
                        style="@style/txtInterbold15"
                        android:layout_width="280dp"
                        android:layout_height="30dp"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="Developpeur Web"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/txtentreprise"
                        style="@style/txtIntermedium12"
                        android:layout_width="280dp"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="CGI"
                        android:textSize="18sp" />

                    <LinearLayout
                        android:id="@+id/linearRowoppeurweb"
                        android:layout_width="201dp"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:orientation="vertical">


                        <LinearLayout
                            android:id="@+id/linearRoweurone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:gravity="start"
                            android:orientation="horizontal">


                            <ImageView
                                android:id="@+id/imageLocalisateurOne"
                                android:layout_width="@dimen/_12pxh"
                                android:layout_height="@dimen/_15pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_localisateur1" />

                            <TextView
                                android:id="@+id/txtregion"
                                style="@style/txtIntermedium12"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_3pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="Paris"
                                android:textSize="14sp" />


                        </LinearLayout>


                        <LinearLayout
                            android:id="@+id/linearRowrevenusone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:gravity="start"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageRevenusOne"
                                android:layout_width="@dimen/_16pxh"
                                android:layout_height="@dimen/_15pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_revenus1" />

                            <TextView
                                android:id="@+id/txtPrice"
                                style="@style/txtIntermedium12"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_3pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="2500"
                                android:textSize="14sp" />
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/eveloppeurweb1"
                    android:layout_width="100dp"
                    android:layout_height="130dp"
                    android:gravity="start"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imagsauvgarder"
                        android:layout_width="26dp"
                        android:layout_height="26dp"
                        android:layout_marginStart="@dimen/_53pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/coeur" />

                    <LinearLayout
                        android:id="@+id/linearRowrusone"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="bottom"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/txtpostuler"
                            android:layout_width="100dp"
                            android:layout_height="30dp"
                            android:layout_gravity="bottom"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:backgroundTint="@color/green_100"
                            android:gravity="center"
                            android:paddingStart="@dimen/_1pxh"
                            android:paddingTop="@dimen/_2pxv"
                            android:paddingBottom="@dimen/_2pxv"
                            android:singleLine="true"
                            android:text="voir plus"
                            android:textColor="@color/black"
                            android:textSize="10sp" />

                    </LinearLayout>

                </LinearLayout>


            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    </LinearLayout>
